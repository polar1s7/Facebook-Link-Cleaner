{
  	"name": "Anti-Annoying link Ranger",
  	"manifest_version": 2,
  	"version": "1.0",
  	"description": "Removes the annoying fb click id ",
	  "browser_action": {
		  "default_icon": "icon_16.png"
  	},
  	"permissions": ["tabs"],
  	"content_scripts": [
  	{
    	"matches": [
      	"*://*.facebook.com/*"
    	],
    	"js": ["jquery-2.2.4.js", "content.js"]
  	}
	]
}